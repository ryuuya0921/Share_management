<h1>新しい投稿</h1>

<%= form_with model: @plaza_post, local: true do |form| %>
  <div>
    <%= form.label :title, 'タイトル' %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :content, '内容' %>
    <%= form.text_area :content %>
  </div>
  <%= form.submit '投稿する', class: 'btn' %>
<% end %>
